mysql> (SELECT p.Stateut,(a.y2006/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3) UNION ALL (SELECT p.Stateut,(a.y2007/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3) UNION ALL (SELECT p.Stateut,(a.y2008/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3) UNION ALL (SELECT p.Stateut,(a.y2009/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3) UNION ALL (SELECT p.Stateut,(a.y2010/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3) UNION ALL (SELECT p.Stateut,(a.y2011/p.Pop2011) FROM population p JOIN accidents a ON (a.StateUts=p.Stateut) WHERE p.Stateut<>'All India' LIMIT 0,3);
